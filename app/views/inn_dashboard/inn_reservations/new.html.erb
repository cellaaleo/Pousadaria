<h1>Nova Reserva</h1>

<%= form_with(model: @inn_reservation, url: inn_dashboard_inns_inn_reservations_path) do |f| %>
  <div class="mb-3">
    <%= f.label :name, class: "form-label" %>
    <%= f.text_field :name, class: "form-control" %>
  </div>
  <div class="mb-3">
    <%= f.label :starting_date, class: "form-label" %>
    <%= f.date_field :starting_date, class: "form-control" %>
  </div>
  <div class="mb-3">
    <%= f.label :ending_date, class: "form-label" %>
    <%= f.date_field :ending_date, class: "form-control" %>
  </div>
  <%= f.submit 'Cadastrar' class: 'btn btn-success' %>
<% end %>